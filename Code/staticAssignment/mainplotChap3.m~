algo_cell = {'JSFP';'GRM';'SAP';'ACO';'PSO';'GA';'SAGA'};
[l,~]=size(algo_cell);
filename = 'TimeTest2/';


C =linspecer(l,'qualitative'); 

% 相同目标数量，不同导弹数量
MaxNm = 50;
Nt = 10;
Nml = (MaxNm-Nt)/10+1;
for i=1:l
    eval([algo_cell{i},'_max=zeros(Nml,100);'])
    eval([algo_cell{i},'_time=zeros(Nml,100);'])
    k=1;
    for Nm = Nt:10:MaxNm
        load([filename,algo_cell{i},'_',num2str(Nm),'vs',num2str(Nt),'_max']);
        eval([algo_cell{i},'_max(k,:)=',algo_cell{i},num2str(Nm),'vs',num2str(Nt),'_max;'])
        load([filename,algo_cell{i},'_',num2str(Nm),'vs',num2str(Nt),'_time']);
        eval([algo_cell{i},'_time(k,:)=',algo_cell{i},num2str(Nm),'vs',num2str(Nt),'_time;'])
        k = k+1;
    end
end


figure(1)
%boxplot(SAP_max');

p(1) = plot(mean(JSFP_max,2)','.-','color',C(1,:),'MarkerSize',15);
hold on

for j=2:3
    %eval(['boxplot(',algo_cell{j},'_max'');']);
    eval(['p(',num2str(j),')=plot(mean(',algo_cell{j},'_max,2)'',''o-'',''color'',C(',num2str(j),',:));'])
end
for j=4:5
    %eval(['boxplot(',algo_cell{j},'_max'');']);
    eval(['p(',num2str(j),')=plot(mean(',algo_cell{j},'_max,2)'',''^-'',''color'',C(',num2str(j),',:));'])
end
for j=6:l
    %eval(['boxplot(',algo_cell{j},'_max'');']);
    eval(['p(',num2str(j),')=plot(mean(',algo_cell{j},'_max,2)'',''*-'',''color'',C(',num2str(j),',:));'])
end
legend(algo_cell)
set(gca,'XTick',1:Nml,'XTickLabel',Nt:10:MaxNm)
xlabel('导弹数量')
ylabel('全局效用平均值（归一化后）')

figure(2)
p(1) = plot(mean(JSFP_time,2)','.-','color',C(1,:),'MarkerSize',15);
hold on

for j=2:3
    %eval(['boxplot(',algo_cell{j},'_max'');']);
    eval(['p(',num2str(j),')=plot(mean(',algo_cell{j},'_time,2)'',''o-'',''color'',C(',num2str(j),',:));'])
end
for j=4:5
    %eval(['boxplot(',algo_cell{j},'_max'');']);
    eval(['p(',num2str(j),')=plot(mean(',algo_cell{j},'_time,2)'',''^-'',''color'',C(',num2str(j),',:));'])
end
for j=6:l
    %eval(['boxplot(',algo_cell{j},'_max'');']);
    eval(['p(',num2str(j),')=plot(mean(',algo_cell{j},'_time,2)'',''*-'',''color'',C(',num2str(j),',:));'])
end
legend(algo_cell)
set(gca,'XTick',1:MaxNm/10,'XTickLabel',Nt:10:MaxNm)
xlabel('导弹数量')
ylabel('平均运行时间/s')

figure(3)


axes('NextPlot','replacechildren','ColorOrder',C); 
Total_max = zeros(100,Nml*l);
for j=1:Nml
    for i=1:l
        Total_max(:,(j-1)*l+i) = eval([algo_cell{i},'_max(',num2str(j),',:)'';']);
        Total_time(:,(i-1)*Nml+j) = eval([algo_cell{i},'_time(',num2str(j),',:)'';']);
    end
end
% boxplot([JSFP_max;GRM_max;SAP_max;PSO_max;GA_max;SAGA_max;ACO_max]');
% tick = {'JSFP','GRM','SAP','PSO','GA','SAGA','ACOSAGA'};
boxplot(Total_max)

set(gca,'XTick',4:l:l*(Nml-1)+4,'XTickLabel',Nt:10:MaxNm);
set(gca,'YGrid','on');
h = findobj(gca,'Tag','Box');
for k=1:Nml
    for j=1:l
        
        box_color((k-1)*l+j)=patch(get(h((k-1)*l+j),'XData'),get(h((k-1)*l+j),'YData'),C(j,:),'FaceAlpha',0.5);
    end
end
legend(box_color(l:-1:1),algo_cell)
% hold on
% plot(1:5,median(JSFP_max,2)','.-','color',C(1,:),'MarkerSize',15,'MarkerFaceColor','k','MarkerEdgeColor','k');

xlabel('导弹数量');
ylabel('全局效用平均值（归一化后）');

%% 时间
figure(4)
boxplot(Total_time)

set(gca,'XTick',3:Nml:Nml*(l-1)+3,'XTickLabel',algo_cell);
set(gca,'YGrid','on');
h = findobj(gca,'Tag','Box');
for k=1:Nml
    for j=1:l
        
        box_color((j-1)*Nml+k)=patch(get(h((j-1)*Nml+k),'XData'),get(h((j-1)*Nml+k),'YData'),C(j,:),'FaceAlpha',0.5);
    end
end
legend(box_color(l:-1:1),num2str(10:10:50)
% hold on
% plot(1:5,median(JSFP_max,2)','.-','color',C(1,:),'MarkerSize',15,'MarkerFaceColor','k','MarkerEdgeColor','k');

xlabel('算法名称');
ylabel('算法运行时间/s');